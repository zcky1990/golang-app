{
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build:css": "npx tailwindcss -i ./static/css/tailwind.css -o ./static/css/tailwind-out.css",
    "build:css:dev": "npx tailwindcss -i ./static/css/tailwind.css -o ./static/dev/css/tailwind-out.css",
    "build:css:prod": "npx tailwindcss -i ./static/css/tailwind.css -o ./static/dist/css/tailwind-out.css --minify",
    "start": "webpack serve --mode development --live-reload",
    "build:prod": "NODE_ENV=production webpack --mode production",
    "build:dev": "NODE_ENV=development webpack --mode development"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "tailwindcss": "^3.4.3",
    "vue-loader": "^17.4.2",
    "vue-template-compiler": "^2.7.16",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "autoprefixer": "^10.4.19",
    "axios": "^1.7.1",
    "postcss": "^8.4.38",
    "vue": "^3.4.27",
    "vue-router": "^4.3.2",
    "vuex": "^4.1.0"
  }
}
